  urlImage       String?          @db.VarChar(200)
  price          Int
  description    String
  category_id    BigInt?
  store_cartitem store_cartitem[]
  store_category store_category?  @relation(fields: [category_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "store_item_category_id_67adee20_fk_store_category_id")

  @@index([category_id], map: "store_item_category_id_67adee20")
